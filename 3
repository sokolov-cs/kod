#include <iostream>  // Подключаем библиотеку для работы с вводом и выводом
using namespace std;

int main()  // Главная функция программы
{
    double i = 12345.6789;  // Объявляем переменную типа double 
    cout << scientific << i << endl; // Выводим значение переменной i
    cout << fixed << i << endl;      // Выводим значение переменной i
    return 0;  // Завершаем программу
}
